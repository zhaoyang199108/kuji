<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuji.dao.LearnExplainDao">
	<insert id="insertIntoLearnExplain" parameterType="com.kuji.entity.LearnExplain">
		insert into learnExplain(learnExplain_category,exercise_id,learnExplain_which_day,
		learnExplain_type,learnExplain_imgPath,
		learnExplain_voicePath,learnExplain_score)
		values(#{category},#{exerciseId},#{whichDay},#{type},#{imgPath},#{voicePath},#{score});
	</insert>
</mapper>