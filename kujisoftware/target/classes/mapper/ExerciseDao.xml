<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.kuji.dao.ExerciseDao">
    <select id="findAllExercise" resultType="ExerciseType" parameterType="int">
    SELECT 
    t.exercise_id,
    t.create_time,
    t.exercise_name 
    FROM `exercise_type` t;
    </select>
    
    </mapper>